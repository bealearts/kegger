{
  "name": "kegger",
  "version": "0.1.1",
  "description": "Homebrew update notification status icon",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager-compile ./ --platform=darwin --arch=x64 --out=dist --icon=./src/assets/keg.icns --overwrite --electron-version=1.7.9",
    "package": "electron-installer-dmg ./dist/kegger-darwin-x64/kegger.app Kegger --out=./dist --icon=./src/assets/keg.icns --overwrite",
    "test": "npm run build && npm run package"
  },
  "keywords": [
    "homebrew",
    "osx",
    "brew"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bealearts/kegger.git"
  },
  "license": "MIT",
  "author": {
    "name": "David Beale",
    "url": "http://bealearts.co.uk/"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^9.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0"
  },
  "dependencies": {
    "electron-log": "^2.2.11",
    "electron-store": "^1.3.0",
    "fs-extra": "^4.0.2",
    "simple-plist": "^0.2.1",
    "temp": "^0.8.3",
    "electron-prebuilt-compile": "^1.7.9"
  }
}
