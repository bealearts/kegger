    "homebrew",
    "osx",
    "brew"
  ],
  "author": "David Beale",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "electron-packager": "^9.1.0",
    "electron-prebuilt-compile": "^1.7.9",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0"
  },
  "dependencies": {
    "electron-log": "^2.2.11",
    "electron-store": "^1.3.0",
    "fs-extra": "^4.0.2",
    "simple-plist": "^0.2.1"
  }
}
