  "license": "MIT",
  "devDependencies": {
    "electron-packager": "^9.1.0",
    "electron-prebuilt": "^1.4.13",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0"
  }
}
