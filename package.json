{
  "name": "kegger",
  "version": "0.0.0",
  "description": "Homebrew update tray icon",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager ./ --platform=darwin --arch=x64 --out=dist --icon=./src/assets/keg.icns --overwrite"
  },
